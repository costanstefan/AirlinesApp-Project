[32m+[m	[32m@Override[m
[32m+[m	[32mpublic void actionPerformed(ActionEvent e)  {[m
[32m+[m
[32m+[m		[32mString FlightCode=flightCode.getText();[m
[32m+[m		[32mString ticketSelectID=(String)ticketID.getSelectedItem();[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m		[32m//BOOK BUTTON[m[41m [m
[32m+[m		[32mif(cancel.equals(e.getSource())) { //cancel button action[m
[32m+[m[41m			[m
[32m+[m			[32mif(TextFieldEmpty()!=0) {[m
[32m+[m				[32mtry {[m
[32m+[m					[32mString date = null;[m
[32m+[m					[32mConnection connectFlightsDB=DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/flights_info", "root","123456");[m
[32m+[m					[32mString query="SELECT * FROM flights WHERE id='"+FlightCode+"'";[m
[32m+[m					[32mStatement st=connectFlightsDB.createStatement();[m
[32m+[m					[32mResultSet rezSet=st.executeQuery(query);[m
[32m+[m					[32mif(rezSet.next()) {[m
[32m+[m						[32m date=rezSet.getString("takeof_date");[m
[32m+[m					[32m}[m
[32m+[m					[32mSystem.out.println(date);[m
[32m+[m[41m					[m
[32m+[m					[32mConnection connectCancelsDB =DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/passagers", "root","123456"); //connection with database[m
[32m+[m					[32mquery= "SELECT * FROM cancellations WHERE ticket_ID='"+ticketSelectID+"'";  // the query which get the compatibility between the inputs values and values from database[m
[32m+[m					[32mPreparedStatement pst= connectCancelsDB.prepareStatement(query); //the statement which have the query[m
[32m+[m					[32mrezSet=pst.executeQuery(); // variable where I pass in the value of the query[m[41m [m
[32m+[m[41m					[m
[32m+[m					[32mif(rezSet.next()) {[m
[32m+[m						[32mJOptionPane.showMessageDialog(null, "This cancellation already exists !");[m
[32m+[m
[32m+[m					[32m}else {[m
[32m+[m						[32mquery ="INSERT INTO cancellations values("+"'0','"+ticketSelectID+"','"+FlightCode+"','"+ date +"')";//insert values into database[m
[32m+[m						[32mst=connectCancelsDB.createStatement();[m
[32m+[m						[32mst.executeUpdate(query);[m
[32m+[m						[32mJOptionPane.showMessageDialog(null, "Ticket Canceled ! ");[m
[32m+[m
[32m+[m
[32m+[m					[32m}[m
[32m+[m[41m					[m
[32m+[m[41m					[m
[32m+[m[41m					[m
[32m+[m[41m					[m
[32m+[m					[32mconnectCancelsDB.close();[m
[32m+[m					[32mconnectFlightsDB.close();[m
[32m+[m						[32mDisplayItems();[m
[32m+[m						[32mClearFields();[m
[32m+[m
[32m+[m[41m					[m
[32m+[m				[32m}catch(Exception e1) {[m
[32m+[m					[32mSystem.out.println("An exception occured: "+e1);[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mSystem.out.println("Text Field Empty");[m
[32m+[m		[32m}[m
[32m+[m[41m			[m
[32m+[m[41m		[m
[32m+[m	[32m}[m
[32m+[m[41m		[m
[32m+[m		[32m//RESET BUTTON[m[41m [m
[32m+[m		[32mif(reset.equals(e.getSource())) {[m
[32m+[m			[32mClearFields();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m		[32m//BACK BUTTON[m
[32m+[m		[32mif(back.equals(e.getSource())) {[m
[32m+[m			[32mClearFields();[m
[32m+[m			[32msetVisible(false);[m
[32m+[m			[32mdispose();[m
[32m+[m			[32mnew FirstPageFrame("MENU");[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m		[32mif(e.getSource()==ticketID) {[m
[32m+[m			[32mGetTicketsData();[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m[32m}[m
