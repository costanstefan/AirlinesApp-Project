[32m+[m	[32m// function for get all passagers IDs from DataBase Id to pass into the ComboBox[m
[32m+[m	[32mprivate static String[] getTicketsId() {[m
[32m+[m		[32mConnection connection;[m
[32m+[m		[32mString query;[m
[32m+[m		[32mStatement st;[m
[32m+[m		[32mResultSet rs;[m
[32m+[m		[32mList<String> values=new ArrayList<String>();[m
[32m+[m		[32mtry {[m
[32m+[m			[32mconnection = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/passagers", "root","123456");[m
[32m+[m			[32mquery= "SELECT ticket_id FROM tickets  ";  // the query which get the passagers Ids[m
[32m+[m			[32mst= connection.createStatement();[m
[32m+[m			[32mrs=st.executeQuery(query);[m
[32m+[m			[32mwhile(rs.next()) {[m
[32m+[m				[32mvalues.add(String.valueOf(rs.getInt("ticket_id")));[m
[32m+[m			[32m}[m
[32m+[m		[32m} catch (SQLException e) {[m
[32m+[m			[32me.printStackTrace();[m
[32m+[m		[32m} //connection with database[m
[32m+[m
[32m+[m			[32mString[] returnValues=new String[values.size()];[m
[32m+[m			[32mvalues.toArray(returnValues);[m
[32m+[m			[32mreturn returnValues;[m
[32m+[m[41m		[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m[41m	[m
[32m+[m	[32mprivate static void GetTicketsData() {[m
[32m+[m		[32mConnection connection;[m
[32m+[m		[32mString query;[m
[32m+[m		[32mStatement st;[m
[32m+[m		[32mResultSet rs;[m
[32m+[m		[32mtry {[m
[32m+[m			[32mconnection = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/passagers", "root","123456");[m
[32m+[m			[32mquery= "SELECT * FROM tickets WHERE ticket_id='"+ticketID.getSelectedItem().toString()+"'";  // the query which get the passagers Ids[m
[32m+[m			[32mst= connection.createStatement();[m
[32m+[m			[32mrs=st.executeQuery(query);[m
[32m+[m			[32mif(rs.next()) {[m
[32m+[m				[32mflightCode.setText(rs.getString("flight_id"));[m
[32m+[m			[32m}[m
[32m+[m		[32m} catch (SQLException e) {[m
[32m+[m			[32me.printStackTrace();[m
[32m+[m		[32m} //connection with database[m
[32m+[m
[32m+[m	[32m}[m
