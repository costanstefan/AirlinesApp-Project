[32m+[m	[32mpublic void DisplayItems() {[m
[32m+[m[41m		[m
[32m+[m				[32mtry {[m
[32m+[m					[32mConnection connection =DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/passagers", "root","123456"); //connection with database[m
[32m+[m					[32mString query= "SELECT * FROM cancellations ";  // the query which get the compatibility between the inputs values and values from database[m
[32m+[m					[32mStatement st= connection.createStatement();[m
[32m+[m					[32mResultSet rs=st.executeQuery(query);[m
[32m+[m					[32mtable.setModel(DbUtils.resultSetToTableModel(rs));[m
[32m+[m
[32m+[m[41m					[m
[32m+[m				[32m}catch(Exception e) {[m
[32m+[m					[32me.printStackTrace();[m
[32m+[m				[32m}[m[41m	[m
[32m+[m		[32m}[m
